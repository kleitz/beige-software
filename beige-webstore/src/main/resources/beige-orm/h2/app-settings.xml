<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>App properties</comment>
<entry key="title">Beige Accounting</entry>
<entry key="databaseVersion">5</entry>
<entry key="appVersion">1.1.4</entry>
<entry key="useSubFolder">h2</entry>
<entry key="jdbcDriverClass">org.h2.Driver</entry>
<entry key="dataSourceClassName">org.h2.jdbcx.JdbcDataSource</entry>
<entry key="databaseName">#currentDir#beigeaccounting</entry>
<entry key="databaseUrl">jdbc:h2:#currentDir#beigeaccounting</entry>
<entry key="userName">beigeaccounting</entry>
<entry key="userPassword">beigeaccounting</entry>
<entry key="checkTableExist">select * from INFORMATION_SCHEMA.TABLES where TABLE_NAME=':tableName';</entry>
</properties>
