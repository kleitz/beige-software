<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>App properties</comment>
<entry key="title">Beige ORM</entry>
<entry key="databaseVersion">1</entry>
<entry key="useSubFolder">postgresql</entry>
<entry key="jdbcDriverClass">org.postgresql.Driver</entry>
<entry key="dataSourceClassName">org.postgresql.ds.PGSimpleDataSource</entry>
<entry key="databaseName">beigeaccounting</entry>
<entry key="userName">beigeaccounting</entry>
<entry key="userPassword">beigeaccounting</entry>
<entry key="databaseUrl">jdbc:postgresql:beigeaccounting</entry>
<entry key="checkTableExist">select * from INFORMATION_SCHEMA.TABLES where TABLE_SCHEMA='public' and TABLE_NAME=':tableName';</entry>
</properties>
